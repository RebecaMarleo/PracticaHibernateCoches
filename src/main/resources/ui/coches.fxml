<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<VBox alignment="CENTER" spacing="20.0" prefHeight="574.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml">
    <children>
        <AnchorPane prefHeight="581.0" prefWidth="600.0">
            <children>
                <Label layoutX="76.0" layoutY="61.0" text="Matrícula" />
                <Label layoutX="76.0" layoutY="96.0" text="Marca" />
                <Label layoutX="76.0" layoutY="131.0" text="Modelo" />
                <Label layoutX="76.0" layoutY="166.0" text="Tipo" />
                <TextField fx:id="txtMatricula" layoutX="143.0" layoutY="57.0" />
                <TextField fx:id="txtMarca" layoutX="143.0" layoutY="92.0" />
                <TextField fx:id="txtModelo" layoutX="143.0" layoutY="127.0" />
                <ComboBox fx:id="cbTipo" layoutX="143.0" layoutY="162.0" prefWidth="150.0" />
                <Button fx:id="btnNuevo" layoutX="76.0" layoutY="217.0" mnemonicParsing="false" onAction="#nuevoCoche" text="Nuevo" />
                <Button fx:id="btnGuardar" layoutX="159.0" layoutY="217.0" mnemonicParsing="false" onAction="#guardarCoche" text="Guardar" />
                <Button fx:id="btnCancelar" layoutX="244.0" layoutY="217.0" mnemonicParsing="false" onAction="#cancelar" text="Cancelar" />
                <Button fx:id="btnModificar" layoutX="336.0" layoutY="217.0" mnemonicParsing="false" onAction="#modificarCoche" text="Modificar" />
                <Button fx:id="btnEliminar" layoutX="433.0" layoutY="217.0" mnemonicParsing="false" onAction="#eliminarCoche" text="Eliminar" />
                <TableView fx:id="tblCoches" layoutX="42.0" layoutY="272.0" onMouseClicked="#seleccionarCoche" prefHeight="231.0" prefWidth="516.0">
                    <columns>
                        <TableColumn fx:id="colMatricula" prefWidth="124.0" text="Matrícula" />
                        <TableColumn fx:id="colMarca" prefWidth="128.0" text="Marca" />
                        <TableColumn fx:id="colModelo" prefWidth="125.0" text="Modelo" />
                        <TableColumn fx:id="colTipo" prefWidth="138.0" text="Tipo" />
                    </columns></TableView>
                <Label fx:id="lblEstado" layoutX="43.0" layoutY="523.0" prefHeight="17.0" prefWidth="516.0" />
            </children>
        </AnchorPane>
    </children>
</VBox>
